    background: #ffffff;
    top: 20px;
    right: 100px;
    width: 50px;
    display: none;
    margin-left: 50px;
    cursor: pointer;

    behavior: url(border-radius.htc);
    -moz-border-radius: 4px;
    -webkit-border-radius: 4px;
    -khtml-border-radius: 4px;
    border-radius: 4px;
}

#more-texts-bar-center{

